# Mempool Monitor Configuration
# Copy this file to .env and fill in your values

# Private key for sending transactions (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Specific transaction hash to target (optional)
TARGET_TX_HASH=0x1234567890abcdef...

# Gas price multiplier (default: 1.1)
GAS_PRICE_MULTIPLIER=1.1

# Recipient address for your transaction
RECIPIENT_ADDRESS=0x1234567890123456789012345678901234567890

# Amount to send in ETH (optional)
SEND_VALUE=0.001
